{
	"layer": "top",
	"position": "top",
	"mod": "dock",
	"height": 36,
	"spacing": 5,
	"margin-top": 3,
	"margin-right": 3,
	"margin-left": 3,
	"exclusive": true,
	"passthrough": false,
	"gtk-layer-shell": true,
	"reload_style_on_change": true,
	"modules-left": [
		"cpu",
		"memory",
	],
	"modules-center": [
		"hyprland/workspaces",
		"hyprland/window",
	],
	"modules-right": [
		"network",
		"pulseaudio",
		"privacy",
		"tray",
		"clock#date",
		"clock#time",
		"custom/rofi",
	],
	"cpu": {
		"interval": 10,
		"format": "Ôíº {usage}%",
		"rotate": 0,
		"format-alt": "{icon0} {icon1} {icon2} {icon3} {icon4} {icon5} {icon6} {icon7}",
		//"format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"]
		"format-icons": [
			"<span color='#69ff94'>‚ñÅ</span>", // green
			"<span color='#2aa9ff'>‚ñÇ</span>", // blue
			"<span color='#f8f8f2'>‚ñÉ</span>", // white
			"<span color='#f8f8f2'>‚ñÑ</span>", // white
			"<span color='#ffffa5'>‚ñÖ</span>", // yellow
			"<span color='#ffffa5'>‚ñÜ</span>", // yellow
			"<span color='#ff9977'>‚ñá</span>", // orange
			"<span color='#dd532e'>‚ñà</span>" // red
		]
	},
	"memory": {
		"states": {
			"c": 90,
			"h": 60,
			"m": 30,
		},
		"interval": 30,
		"format": "Û∞æÜ {used}GB",
		"rotate": 0,
		"format-m": "Û∞æÖ {used}GB",
		"format-h": "Û∞ìÖ {used}GB",
		"format-c": "Ôê° {used}GB",
		"format-alt": "Û∞æÜ {percentage}%",
		"max-length": 10,
		"tooltip": true,
		"tooltip-format": "Û∞æÜ {percentage}%\nÓâ¶ {used:0.1f}GB/{total:0.1f}GB"
	},
	"hyprland/workspaces": {
		//"disable-scroll": true,
		//"active-only": false,
		//"all-outputs": true,
		//"warp-on-scroll": false,
		"format": "{icon}",
		//"format": "<sub>{icon}</sub>\n{windows}",
		"format-icons": {
			"1": "<span size='xx-large' color='#cc241d' >Û∞é¶Û∞ä†</span>",
			"2": "<span size='xx-large' color='#9797ae' >Û∞é©Û∞ä†</span>",
			"3": "<span size='xx-large' color='#d79921' >Û∞é¨Ôáø</span>",
			"4": "<span size='xx-large' color='#458588' >Û∞éÆÛ∞ä†</span>",
			"5": "<span size='xx-large' color='#b16286' >Û∞é∞Û∞ä†</span>",
			"6": "<span size='xx-large' color='#689d6a' >Û∞éµÛ∞ä†</span>",
			"7": "<span size='xx-large' color='#fe8019' >Û∞é∏Û∞ä†</span>",
			"8": "<span size='xx-large' color='#83a598' >Û∞éªÛ∞ä†</span>",
			//"9": "<span size='xx-large' color='#d3869b' >Û∞éæÔÜ∂</span>",
			"9": "<span size='xx-large' color='#FFB8FF' >Û∞éæÔÜ∂</span>",
			"10": "<span size='xx-large' color='#FFB8FF' >Û∞ΩæÛ∞äó</span>",
			"active": "<span size='xx-large' color='#ffff00' >Û∞ÆØ</span>",
			//"default": "<span color='#cc241d'>Û∞ä†</span>",
		},
		//"persistent-workspaces": {
		//	"*": 10
		//}
	},
	"hyprland/window": {
		//"format": "ÔÇ§ {}",
		"format": "{class}",
		"max-length": 20,
		"rewrite": {
			"": "<span foreground= '#458588'>Ôçô</span> hyprland",
			"~": "ÓØä terminal",
			"com.mitchellh.ghostty": "ghostty",
		}
		//"rewrite": {
		//	"(.*) ‚Äî Zen Browser": "Óù´ üåé $1",
		//	"(.*) - fish": "> [$1]"
		//},
		//"separate-outputs": true
	},
	"network": {
		"tooltip": true,
		//"format-wifi": "Ôá´ ",
		"format": "{ifname}",
		"format-wifi": "Ôá´ {essid} ({signalStrength}%)",
		"rotate": 0,
		"format-ethernet": "Û∞àÄ ",
		"tooltip-format": "Network: <big><b>{essid}</b></big>\nSignal strength: <b>{signaldBm}dBm ({signalStrength}%)</b>\nFrequency: <b>{frequency}MHz</b>\nInterface: <b>{ifname}</b>\nIP: <b>{ipaddr}/{cidr}</b>\nGateway: <b>{gwaddr}</b>\nNetmask: <b>{netmask}</b>",
		"format-linked": "Û∞àÄ {ifname} (No IP)",
		"format-disconnected": "Û∞ñ™ ",
		"tooltip-format-disconnected": "Disconnected",
		"format-alt": "<span foreground='#99ffdd'>ÔÄô {bandwidthDownBytes}</span> <span foreground='#ffcc66'>ÔÇì {bandwidthUpBytes}</span>",
		"interval": 2,
	},
	"pulseaudio": {
		"tooltip": false,
		"scroll-step": 5,
		"format": "<span color='#fbf1c7' > ÔÄ® </span>{volume}%",
		"format-muted": "<span color='#fbf1c7' > Óª® </span>{volume}%",
		"format-bluetooth": "<span color='#fbf1c7' > Û∞Ç∞ </span>{volume}%",
		"format-bluetooth-muted": "<span color='#fbf1c7' > Û∞Ç≤ </span>{volume}%",
		"format-source": "{volume}% ÔÑ∞",
		"on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
		"max-volume": 100
	},
	"tray": {
		"icon-size": 22,
		"spacing": 5,
		"show-passive-items": true,
	},
	"clock#date": {
		"format": "<span color='#8ec07c' >Û∞É≠ </span>{:%a %d %b}",
		"tooltip-format": "<tt>{calendar}</tt>",
		//                      "months":     "<span color='#ffead3'><b>{}</b></span>",
		//                      "days":       "<span color='#ecc6d9'><b>{}</b></span>",
		//                      "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
		//                      "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
		//                      "today":      "<span color='#ff6699'><b><u>{}</u></b></span>"
		"calendar": {
			"mode": "month",
			"mode-mon-col": 3,
			"weeks-pos": "right",
			"on-scroll": 1,
			"on-click-right": "mode",
			"format": {
				"months": "<span color='#3c3835'><b>{}</b></span>",
				"weeks": "<span color='#fe8019'><b>W{}</b></span>",
				"weekdays": "<span color='#fe8019'>{}</span>",
				"today": "<span color='#cc241d'><b>{}</b></span>"
			}
		},
		"actions": {
			"on-click": "mode",
			"on-click-right": "mode",
			"on-scroll-up": "tz_up",
			"on-scroll-down": "tz_down",
			"on-scroll-up": "shift_up",
			"on-scroll-down": "shift_down"
		},
		"min-length": 8,
		"max-length": 20
	},
	"clock#time": {
		//"format": "<span color='#cc241d' >Û±ëÇ </span>{:%I:%M %p}",
		"format": "<span color='#cc241d' >Û±ëÇ </span>{:%I:%M:%S %p (%Z)}",
		//"tooltip": false,
		// "tooltip-format": "Standard Time: {:%I:%M %p}",
		"tooltip-format": "{tz_list}",
		"timezones": [
			"Europe/Amsterdam",
			"Etc/UTC",
			"Europe/London",
		],
		"interval": 1,
		//"min-length": 8,
		//"max-length": 12
	},
	"custom/rofi": {
		"format": "<span color='#fbf1c7' >Û∞ÆØ</span>",
		"on-click": "rofi -show drun",
		"on-click-right": "wlogout",
		"escape": true,
		"tooltip": false
	},
}
